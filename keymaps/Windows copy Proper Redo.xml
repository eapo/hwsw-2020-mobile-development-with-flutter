<keymap version="1" name="Windows copy Proper Redo" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="$Undo">
    <keyboard-shortcut first-keystroke="ctrl z" />
    <keyboard-shortcut first-keystroke="alt back_space" />
    <keyboard-shortcut first-keystroke="shift ctrl y" />
  </action>
  <action id="CheckinProject" />
  <action id="CommentByBlockComment">
    <keyboard-shortcut first-keystroke="shift ctrl slash" />
    <keyboard-shortcut first-keystroke="shift ctrl divide" />
    <keyboard-shortcut first-keystroke="shift ctrl k" />
  </action>
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="ctrl slash" />
    <keyboard-shortcut first-keystroke="ctrl divide" />
    <keyboard-shortcut first-keystroke="ctrl k" />
  </action>
  <action id="DecrementWindowHeight" />
  <action id="Editor Undo">
    <keyboard-shortcut first-keystroke="shift ctrl y" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="EditorStartNewLineBefore" />
  <action id="Flutter.Toolbar.ReloadAction">
    <keyboard-shortcut first-keystroke="ctrl back_slash" />
    <keyboard-shortcut first-keystroke="shift alt enter" />
    <keyboard-shortcut first-keystroke="ctrl alt enter" />
  </action>
  <action id="IncrementWindowHeight" />
  <action id="MoveLineDown">
    <keyboard-shortcut first-keystroke="shift alt down" />
    <keyboard-shortcut first-keystroke="shift ctrl down" />
  </action>
  <action id="MoveLineUp">
    <keyboard-shortcut first-keystroke="shift alt up" />
    <keyboard-shortcut first-keystroke="shift ctrl up" />
  </action>
  <action id="MoveStatementDown" />
  <action id="MoveStatementUp" />
  <action id="QuickActionPopup" />
  <action id="ResizeToolWindowDown" />
  <action id="ResizeToolWindowUp" />
  <action id="Vcs.Push" />
</keymap>